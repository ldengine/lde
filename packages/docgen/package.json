{
  "name": "@lde/docgen",
  "version": "0.1.0",
  "description": "Generate documentation from SHACL shapes",
  "keywords": [
    "shacl",
    "rdf",
    "documentation",
    "generator",
    "semantic-web"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "frames",
    "!**/*.tsbuildinfo"
  ],
  "bin": {
    "docgen": "./dist/cli.js"
  },
  "dependencies": {
    "commander": "^14.0.0",
    "jsonld": "^8.3.3",
    "liquidjs": "^10.21.1",
    "rdf-dereference": "^4.0.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@types/jsonld": "^1.5.15"
  }
}
