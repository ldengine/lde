PREFIX void: <http://rdfs.org/ns/void#>
PREFIX void-ext: <http://ldf.fi/void-ext#>

CONSTRUCT {
    ?dataset a void:Dataset ;
        void-ext:distinctLiterals ?distinctLiterals .
}
WHERE {
    SELECT (COUNT(DISTINCT ?o) AS ?distinctLiterals) {
        #subjectFilter#
        ?s ?p ?o .
        FILTER(ISLITERAL(?o))
    }
}
